<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".navFragment.MapFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_gameList"
            android:layout_width="wrap_content"
            android:layout_height="220dp"
            android:layout_margin="16dp"
            android:elevation="12dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="16dp"
            app:cardElevation="8dp"
            app:cardUseCompatPadding="false">

            <RelativeLayout
                android:id="@+id/bin1"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/square1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="24dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/typeofbin1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Harvey Norman Djitsun Mall"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/typeofbin1"
                        android:layout_marginBottom="170dp"
                        android:text="5 Ang Mo Kio Central 2, S(569663)"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/how"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="100dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="How"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/full"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="50dp"
                        android:layout_marginTop="100dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Full"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/thebin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="90dp"
                        android:layout_marginTop="100dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Is the Bin?"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="40dp"
                        android:layout_below="@id/full"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/bar" />

                    <ImageView
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:layout_below="@id/full"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/bar2" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/full"
                        android:layout_marginLeft="240dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="63%"
                        android:textColor="#fff"
                        android:textSize="30dp" />


                </RelativeLayout>


            </RelativeLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_below="@id/cardView_gameList"
            android:layout_width="wrap_content"
            android:layout_height="220dp"
            android:layout_margin="16dp"
            android:elevation="12dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="16dp"
            app:cardElevation="8dp"
            app:cardUseCompatPadding="false">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/square2">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="24dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/typeofbin2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="ICT, Batteries and Bulbs Bin 2"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/typeofbin2"
                        android:layout_marginBottom="170dp"
                        android:text="61A Strathmore Ave"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/how2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="100dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="How"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/full2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="50dp"
                        android:layout_marginTop="100dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Full"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/thebin2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="90dp"
                        android:layout_marginTop="100dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Is the Bin?"
                        android:textColor="@color/darkgreen"
                        android:textSize="20dp" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="40dp"
                        android:layout_below="@id/full2"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/bar" />

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="40dp"
                        android:layout_below="@id/full2"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/bar2" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/full2"
                        android:layout_marginLeft="240dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="32%"
                        android:textSize="30dp"
                        android:textColor="#fff"/>
                </RelativeLayout>

            </RelativeLayout>

        </androidx.cardview.widget.CardView>
    </RelativeLayout>



</FrameLayout>